	file = {ACM Full Text PDF:/Users/torres/Library/Application Support/Zotero/Profiles/bvdd7rjn.default/zotero/storage/P3CZ4SA7/Jung et al. - 2014 - Participatory Materials Having a Reflective Conve.pdf:application/pdf}
