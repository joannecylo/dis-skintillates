	file = {ACM Full Text PDF:/Users/torres/Library/Application Support/Zotero/Profiles/bvdd7rjn.default/zotero/storage/A47EXN73/Terry and Mynatt - 2002 - Recognizing Creative Needs in User Interface Desig.pdf:application/pdf}
}

