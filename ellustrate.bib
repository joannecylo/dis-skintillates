	file = {ACM Full Text PDF:/Users/torres/Library/Application Support/Zotero/Profiles/bvdd7rjn.default/zotero/storage/SKEISVVS/Zoran et al. - 2014 - The Hybrid Artisans A Case Study in Smart Tools.pdf:application/pdf}
}




